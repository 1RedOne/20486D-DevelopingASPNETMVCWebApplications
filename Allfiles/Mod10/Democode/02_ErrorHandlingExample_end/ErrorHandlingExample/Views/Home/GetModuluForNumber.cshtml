@inject ErrorHandlingExample.Services.ICounter cnt
@using Microsoft.AspNetCore.Http.Extensions
@model ErrorHandlingExample.Models.DivisionResult
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetModuluForNumber</title>
    <link href="css\Style.css" rel="stylesheet" type="text/css" />
</head>
<body>
    The @Model.DividedNumber number, is divided by the following numbers: @Model.ToString()<br />
    This number was searched @cnt.UrlCounter[Context.Request.GetDisplayUrl()] times<br />
    @Html.Partial("_NumberLinks")
</body>
</html>
